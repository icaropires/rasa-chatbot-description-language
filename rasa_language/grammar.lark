start  : blocks

blocks : block+

block :  header topics _BREAK_LINE*

header : "[" NAME ": " NAME "]" _BREAK_LINE

topics : ("- " (word " "?)+ _BREAK_LINE)+ 

synonym : "{" NAME ":" (NAME "|"?)+ "}"

word : TOPIC
		  | synonym


TOPIC : /[\w\[\]\-\+\_\?\!\:\;]+/
_BREAK_LINE : /\n/
NAME: /[\w\d\-_]+/
SPACE: /" "/
