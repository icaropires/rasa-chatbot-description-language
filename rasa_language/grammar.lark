start  : blocks

blocks : block+

block :  header topics _BREAK_LINE*

header : "[" NAME ": " NAME "]" _BREAK_LINE

topics : topic+

topic : MARKER _SPACE (element _SPACE*)+ _BREAK_LINE

synonym : "{" NAME "}{" NAME ":" (NAME "|"?)+ "}"

element: text 
	  | synonym

text : TEXT

MARKER : /[>-]/
TEXT : /[\w\[\]\-+_?!:;,. ]+/
NAME: /[\w\d\-_]+/
_BREAK_LINE : /\n/
_SPACE: " "
